package com.github.namespace

class Entity {
	@ exportAs: "pos"
	Vector3 position

	@ {
		exportAs: "rot",
	}
	Quaternion rotation
}

class Player : Ent { // this line is wrong since there is no declaration of type Ent
	@range: [0, 5]
	int state

	@range: [-2.333, 2^8>
	float progress
}

struct Vector3 {
	float x, y, z
}

struct Quaternion {
	@range: [0, sqrt(4)*(7+3)*8/2]
	@precision: e^pi
	float i, j, k, w
}